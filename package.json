{
	"name": "omninext",
	"version": "1.0.0",
	"description": "Simple AWS Lambda REST API in Node.js with Serverless",
	"scripts": {
		"init": "sls dynamodb install",
		"deploy": "sls deploy --stage prod",
		"undeploy": "sls remove --stage prod",
		"dev": "sls offline start --stage dev",
		"format": "prettier \"src/**/*.ts\" --write --config .prettierrc",
		"lint": "eslint --fix ."
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.515.0",
		"@aws-sdk/lib-dynamodb": "^3.515.0",
		"aws-lambda": "^1.0.7",
		"http-status-codes": "^2.3.0",
		"serverless-dynamodb": "^0.2.50",
		"serverless-http": "^3.1.1",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@types/aws-lambda": "^8.10.134",
		"@types/node": "^20.11.19",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"esbuild": "^0.20.1",
		"eslint": "^8.56.0",
		"prettier": "^3.2.5",
		"serverless-esbuild": "^1.51.0",
		"serverless-offline": "^13.3.3",
		"typescript": "^5.3.3"
	}
}
